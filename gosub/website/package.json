    "astro": "^5.0.3",
    "gosub-goproxy": "workspace:*",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "solid-js": "^1.9.3",
    "typescript": "^5.7.2",
    "utf64": "workspace:^",
    "vitest": "^2.1.8",
    "wrangler": "^3.92.0"
  }
}
