{
  "name": "gosub.website",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "prettier --log-level warn -w .",
    "lint": "astro check",
    "test": "vitest --pool=threads run",
    "dev": "astro dev",
    "build": "astro build",
    "push": "wrangler deploy"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-astro"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro",
          "printWidth": 1000
        }
      }
    ]
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.5.2",
    "@astrojs/solid-js": "^5.0.9",
    "@moreplease/rollup-plugin-build-info": "workspace:*",
    "astro": "^5.7.8",
    "gosub-goproxy": "workspace:*",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "solid-js": "^1.9.5",
    "typescript": "^5.8.3",
    "utf64": "workspace:^",
    "vitest": "^3.1.2",
    "wrangler": "^4.13.2"
  }
}
