{
  "compilerOptions": {
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true,
    "target": "es2015",
    "module": "es2020",
    "lib": ["es2015"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "outDir": "build"
  },
  "files": ["utf64.ts", "utf64.tool.ts"]
}
