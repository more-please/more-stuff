{
  "name": "utf64.website",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "prettier --log-level warn -w .",
    "lint": "astro check",
    "test": "vitest --pool=threads run",
    "dev": "astro dev",
    "build": "astro build",
    "push": "wrangler deploy"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-astro"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro",
          "printWidth": 1000
        }
      }
    ]
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/solid-js": "^5.1.3",
    "@moreplease/rollup-plugin-build-info": "workspace:*",
    "astro": "^5.16.8",
    "gosub-goproxy": "workspace:*",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.9.3",
    "utf64": "workspace:*",
    "vitest": "^4.0.16",
    "wrangler": "^4.59.2"
  }
}
